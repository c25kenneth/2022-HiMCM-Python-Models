      "cell_type": "code",
      "source": [
        "doc = nlp(report_df[\"report_path\"][3])\n",
        "displacy.render(doc, style=\"ent\", jupyter=True)"
      ],
      "metadata": {
        "id": "5nYD5OeUSgiL"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import tqdm\n",
        "just_text = [i for i in report_df[\"report_path\"]]\n",
        "docs = list(nlp.pipe(just_text))"
      ],
      "metadata": {
        "id": "5ekwMLbIXSL7"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "distances = []\n",
        "for d in docs:\n",
        "    closest_quantity = None\n",
        "    min_distance = float(\"inf\")\n",
        "    found_quantity = False\n",
        "    for ent in d.ents:\n",
        "        if ent.label_ != \"QUANTITY\":\n",
        "            continue\n",
        "        if re.search(\"carina|cm|tube\", ent.sent.text):\n",
        "            distance = min(abs(ent.sent.text.find(\"carina\")), abs(ent.sent.text.find(\"cm\")), abs(ent.sent.text.find(\"tube\")))\n",
        "            if (distance < min_distance):\n",
        "                closest_quantity = ent.text\n",
        "                min_distance = distance\n",
        "                found_quantity = True\n",
        "    if found_quantity:\n",
        "        if \"mm\" in closest_quantity:\n",
        "              closest_quantity = None\n",
        "        else:\n",
        "            closest_quantity = closest_quantity.replace(\"approximately\", \"\").replace(\"about\", \"\").replace(\"above\", \"\").replace(\"below\", \"\").replace(\"\\\\n\", \"\").replace(\"cm\", \"\").replace(\" \", \"\").replace(\"centimeters\", \"\").replace(\"mm\", \"\").replace(\"'\", \"\").replace(\"some\", \"\").replace(\"atleast\", \"\").replace(\"to\", \"\")\n",
        "            try:\n",
        "                closest_quantity = float(closest_quantity)\n",
        "            except ValueError:\n",
        "                closest_quantity = None\n",
        "    distances.append(closest_quantity)"
      ],
      "metadata": {
        "id": "fa4O_I_kWwpE"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "report_df = report_df[report_df['distances'].notna()]\n",
        "report_df"
      ],
      "metadata": {
        "id": "isjLj6YZaFDV"
